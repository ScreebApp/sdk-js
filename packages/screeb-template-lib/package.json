{
  "name": "@screeb/template-lib",
  "version": "0.1.2",
  "author": "Screeb's frontend team",
  "description": "Screeb's template for a front-end lib.",
  "private": true,
  "license": "MIT",
  "scripts": {
    "lint": "eslint .",
    "build": "rollup -c ./node_modules/@screeb/typescript-config/src/rollup.config.js",
    "clean": "rm -Rf dist"
  },
  "sideEffects": false,
  "type": "module",
  "module": "dist/es/index.js",
  "types": "dist/es/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": "./dist/es/index.js"
    }
  },
  "dependencies": {
    "@screeb/eslint-config": "^0.1.2",
    "@screeb/typescript-config": "^0.1.2",
    "@types/node": "^18.16.3",
    "@typescript-eslint/eslint-plugin": "^5.59.2",
    "eslint": "^8.39.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.5.0",
    "prettier": "^2.8.8",
    "rollup": "^3.21.4"
  }
}
