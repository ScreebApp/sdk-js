{
  "name": "@screeb/sdk-browser",
  "version": "0.1.3",
  "author": "Screeb's frontend team",
  "description": "Screeb's browser sdk.",
  "license": "MIT",
  "scripts": {
    "lint": "eslint .",
    "build": "rollup -c ./node_modules/@screeb/typescript-config/src/rollup.config.js",
    "build:doc": "typedoc src/index.ts",
    "clean": "rm -Rf dist",
    "test": "jest"
  },
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": false,
  "type": "module",
  "module": "dist/es/index.js",
  "types": "dist/es/index.d.ts",
  "files": [
    "dist",
    "docs",
    "README.md"
  ],
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": "./dist/es/index.js"
    }
  },
  "dependencies": {
    "@screeb/eslint-config": "^0.1.2",
    "@screeb/typescript-config": "^0.1.2",
    "@types/jest": "^29.5.1",
    "@types/node": "^18.16.3",
    "@typescript-eslint/eslint-plugin": "^5.59.2",
    "eslint": "^8.39.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "prettier": "^2.8.8",
    "rollup": "^3.21.4",
    "ts-jest": "^29.1.0",
    "typedoc": "^0.24.6",
    "typedoc-plugin-markdown": "^3.15.3"
  }
}
